<header>
  <h1><%=@album.name%></h1>
</header>
<div class="container" id="main">
  <div class="container" id="pics">
    <% @album.images.each do |image| %>
        <div>
          <a href="<%="/projects/#{@album.name}/images/#{image.id}"%>"><%=cl_image_tag(image.external_id, :height => 150, :crop => "pad")%></a>
        </div>
    <% end %>
  </div>
</div>